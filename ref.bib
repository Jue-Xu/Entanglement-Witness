@article{aaronsonQuantumLowerBounds2004,
  title = {Quantum Lower Bounds for the Collision and the Element Distinctness Problems},
  author = {Aaronson, Scott and Shi, Yaoyun},
  year = {2004},
  month = jul,
  journal = {J. ACM},
  volume = {51},
  number = {4},
  eprint = {quant-ph/0112086},
  eprinttype = {arxiv},
  pages = {595--605},
  issn = {0004-5411},
  doi = {10.1145/1008731.1008735},
  url = {https://doi.org/10.1145/1008731.1008735},
  urldate = {2020-12-16},
  abstract = {Given a function f as an oracle, the collision problem is to find two distinct indexes i and j such that f(i) = f(j), under the promise that such indexes exist. Since the security of many fundamental cryptographic primitives depends on the hardness of finding collisions, our lower bounds provide evidence for the existence of cryptographic primitives that are immune to quantum cryptanalysis. We prove that any quantum algorithm for finding a collision in an r-to-one function must evaluate the function {$\Omega$}((n/r)1/3) times, where n is the size of the domain and r|n. This matches an upper bound of Brassard, H\o yer, and Tapp. No lower bound better than constant was previously known. Our result also implies a quantum lower bound of {$\Omega$}(n2/3) queries for the element distinctness problem, which is to determine whether n integers are all distinct. The best previous lower bound was {$\Omega$}({$\surd$}n) queries.},
  archiveprefix = {arXiv},
  keywords = {Collision,element distinctness,polynomial method,quantum computing,quantum lower bounds}
}

@misc{aaronsonQuantumLowerBounds2020,
  title = {Quantum {{Lower Bounds}} for {{Approximate Counting}} via {{Laurent Polynomials}}},
  author = {Aaronson, Scott and Kothari, Robin and Kretschmer, William and Thaler, Justin},
  year = {2020},
  month = jun,
  eprint = {1904.08914},
  eprinttype = {arxiv},
  primaryclass = {quant-ph},
  url = {http://arxiv.org/abs/1904.08914},
  urldate = {2020-10-24},
  abstract = {This paper proves new limitations on the power of quantum computers to solve approximate counting\textemdash that is, multiplicatively estimating the size of a nonempty set S {$\subseteq$} [N ]. Given only a membership oracle for S, it is well known that approximate counting takes {$\Theta$} N/|S| quantum queries. But what if a quantum algorithm is also given ``QSamples''\textemdash i.e., copies of the state |S = i{$\in$}S |i \textemdash or even the ability to apply reflections about |S ? Our first main result is that, even then, the algorithm needs either {$\Theta$} N/ |S| queries or else {$\Theta$} min |S|1/3 , N/ |S| reflections or samples. We also give matching upper bounds.},
  archiveprefix = {arXiv},
  langid = {english},
  keywords = {Computer Science - Computational Complexity,Quantum Physics}
}

@inproceedings{aaronsonQuantumSearchSpatial2003,
  title = {Quantum Search of Spatial Regions},
  booktitle = {44th {{Annual IEEE Symposium}} on {{Foundations}} of {{Computer Science}}, 2003. {{Proceedings}}.},
  author = {Aaronson, S. and Ambainis, A.},
  year = {2003},
  month = oct,
  eprint = {quant-ph/0303041},
  eprinttype = {arxiv},
  pages = {200--209},
  issn = {0272-5428},
  doi = {10.1109/SFCS.2003.1238194},
  url = {https://arxiv.org/abs/quant-ph/0303041},
  abstract = {Can Grover's quantum search algorithm speed up search of a physical region - for example a 2D grid of size /spl radic/n x /spl radic/n? The problem is that /spl radic/n time seems to be needed for each query, just to move amplitude across the grid. Here we show that this problem can be surmounted, refuting a claim to the contrary by Benioff. In particular, we show how to search a d-dimensional hypercube in time 0(/spl radic/n) for d /spl ges/ 3, or 0(/spl radic/n log/sup 3/ n) for d = 2. More generally, we introduce a model of quantum query complexity on graphs, motivated by fundamental physical limits on information storage, particularly the holographic principle from black hole thermodynamics. Our results in this model include almost-tight upper and lower bounds for many search tasks; a generalized algorithm that works for any graph with good expansion properties, not just hypercubes; and relationships among several notions of locality for unitary matrices acting on graphs. As an application of our results, we give an 0(/spl radic/n)-qubit communication protocol for the disjointness problem, which improves an upper bound of Hoyer and de Wolf and matches a lower bound of Razborov.},
  archiveprefix = {arXiv},
  keywords = {0(/spl radic/n)-qubit communication protocol,black hole thermodynamics,computational complexity,Databases,disjointness problem,generalized algorithm,graph theory,Holography,Hypercubes,information storage,Laboratories,Orbital robotics,Physics,Protocols,quantum computing,quantum query complexity,quantum search algorithm,query processing,Robots,search problems,spatial region,Thermodynamics,unitary matrix,Upper bound}
}

@inproceedings{aaronsonShadowTomographyQuantum2018,
  title = {Shadow Tomography of Quantum States},
  booktitle = {Proc. 50th {{Annu}}. {{ACM SIGACT Symp}}. {{Theory Comput}}.},
  author = {Aaronson, Scott},
  year = {2018},
  month = jun,
  series = {{{STOC}} 2018},
  eprint = {1711.01053},
  eprinttype = {arxiv},
  pages = {325--338},
  publisher = {{Association for Computing Machinery}},
  address = {{New York, NY, USA}},
  doi = {10.1145/3188745.3188802},
  url = {https://doi.org/10.1145/3188745.3188802},
  urldate = {2022-07-24},
  abstract = {We introduce the problem of *shadow tomography*: given an unknown D-dimensional quantum mixed state {$\rho$}, as well as known two-outcome measurements E1,\ldots,EM, estimate the probability that Ei accepts {$\rho$}, to within additive error {$\epsilon$}, for each of the M measurements. How many copies of {$\rho$} are needed to achieve this, with high probability? Surprisingly, we give a procedure that solves the problem by measuring only O( {$\epsilon-$}5{$\cdot$}log4 M{$\cdot$}logD) copies. This means, for example, that we can learn the behavior of an arbitrary n-qubit state, on *all* accepting/rejecting circuits of some fixed polynomial size, by measuring only nO( 1) copies of the state. This resolves an open problem of the author, which arose from his work on private-key quantum money schemes, but which also has applications to quantum copy-protected software, quantum advice, and quantum one-way communication. Recently, building on this work, Brand\~ao et al. have given a different approach to shadow tomography using semidefinite programming, which achieves a savings in computation time.},
  archiveprefix = {arXiv},
  isbn = {978-1-4503-5559-9},
  keywords = {information theory,mixed state,one-way communication,postselection,quantum advice,quantum money}
}

