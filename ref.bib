  eprinttype = {arxiv},
  pages = {372--383},
  publisher = {{Springer}},
  address = {{Berlin, Heidelberg}},
  doi = {10.1007/978-3-540-30538-5_31},
  abstract = {The k-Local Hamiltonian problem is a natural complete problem for the complexity class QMA, the quantum analog of NP. It is similar in spirit to MAX- k -SAT, which is NP-complete for k {$\geq$} 2. It was known that the problem is QMA-complete for any k {$\geq$} 3. On the other hand 1-Local Hamiltonian is in P, and hence not believed to be QMA-complete. The complexity of the 2-Local Hamiltonian problem has long been outstanding. Here we settle the question and show that it is QMA-complete. We provide two independent proofs; our first proof uses a powerful technique for analyzing the sum of two Hamiltonians; this technique is based on perturbation theory and we believe that it might prove useful elsewhere. The second proof uses elementary linear algebra only. Using our techniques we also show that adiabatic computation with two-local interactions on qubits is equivalent to standard quantum computation.},
  archiveprefix = {arXiv},
  isbn = {978-3-540-30538-5},
  langid = {english},
  keywords = {Computer Science - Computational Complexity,Quantum Physics}
}

