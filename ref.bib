}

@article{huangEfficientEstimationPauli2021,
  title = {Efficient Estimation of {{Pauli}} Observables by Derandomization},
  author = {Huang, Hsin-Yuan and Kueng, Richard and Preskill, John},
  year = {2021},
  month = jul,
  journal = {Phys. Rev. Lett.},
  volume = {127},
  number = {3},
  eprint = {2103.07510},
