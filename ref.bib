@misc{rajputHybridizedMethodsQuantum2021,
  title = {Hybridized {{Methods}} for {{Quantum Simulation}} in the {{Interaction Picture}}},
  author = {Rajput, Abhishek and Roggero, Alessandro and Wiebe, Nathan},
