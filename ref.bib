@misc{szegedySpectraQuantizedWalks2004,
  title = {Spectra of {{Quantized Walks}} and a \$\textbackslash sqrt\{\textbackslash delta\textbackslash epsilon\}\$ Rule},
  author = {Szegedy, Mario},
  year = {2004},
  month = jan,
  eprint = {quant-ph/0401053},
  eprinttype = {arxiv},
  url = {http://arxiv.org/abs/quant-ph/0401053},
  urldate = {2021-01-31},
  abstract = {We introduce quantized bipartite walks, compute their spectra, generalize the algorithms of Grover \textbackslash cite\{g\} and Ambainis \textbackslash cite\{amb03\} and interpret them as quantum walks with memory. We compare the performance of walk based classical and quantum algorithms and show that the latter run much quicker in general. Let \$P\$ be a symmetric Markov chain with transition probabilities \$P[i,j]\$, \$(i ,j\textbackslash in [n])\$. Some elements of the state space are marked. We are promised that the set of marked elements has size either zero or at least \$\textbackslash epsilon n\$. The goal is to find out with great certainty which of the above two cases holds. Our model is a black box that can answer certain yes/no questions and can generate random elements picked from certain distributions. More specifically, by request the black box can give us a uniformly distributed random element for the cost of \$\textbackslash wp\_\{0\}\$. Also, when ``inserting'' an element \$i\$ into the black box we can obtain a random element \$j\$, where \$j\$ is distributed according to \$P[i,j]\$. The cost of the latter operation is \$\textbackslash wp\_\{1\}\$. Finally, we can use the black box to test if an element \$i\$ is marked, and this costs us \$\textbackslash wp\_\{2\}\$. If \$\textbackslash delta\$ is the eigenvalue gap of \$P\$, there is a simple classical algorithm with cost \$O(\textbackslash wp\_\{0\} + (\textbackslash wp\_\{1\}+\textbackslash wp\_\{2\})/\textbackslash delta\textbackslash epsilon)\$ that solves the above promise problem. (The algorithm is efficient if \$\textbackslash wp\_\{0\}\$ is much larger than \$\textbackslash wp\_\{1\}+\textbackslash wp\_\{2\}\$.) In contrast,we show that for the ``quantized'' version of the algorithm it costs only \$O(\textbackslash wp\_\{0\} + (\textbackslash wp\_\{1\}+\textbackslash wp\_\{2\})/\textbackslash sqrt\{\textbackslash delta\textbackslash epsilon\})\$ to solve the problem. We refer to this as the \$\textbackslash sqrt\{\textbackslash delta\textbackslash epsilon\}\$ rule. Among the technical contributions we give a formula for the spectrum of the product of two general reflections.},
  archiveprefix = {arXiv},
  langid = {english},
  keywords = {Quantum Physics}
}

