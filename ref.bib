@article{banulsSimulatingLatticeGauge2020,
  title = {Simulating {{Lattice Gauge Theories}} within {{Quantum Technologies}}},
  author = {Ba{\~n}uls, M. C. and Blatt, R. and Catani, J. and Celi, A. and Cirac, J. I. and Dalmonte, M. and Fallani, L. and Jansen, K. and Lewenstein, M. and Montangero, S. and Muschik, C. A. and Reznik, B. and Rico, E. and Tagliacozzo, L. and Van Acoleyen, K. and Verstraete, F. and Wiese, U.-J. and Wingate, M. and Zakrzewski, J. and Zoller, P.},
