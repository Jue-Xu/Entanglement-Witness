@misc{dawsonSolovayKitaevAlgorithm2005,
  title = {The {{Solovay-Kitaev}} Algorithm},
  author = {Dawson, Christopher M. and Nielsen, Michael A.},
  year = {2005},
  month = aug,
  eprint = {quant-ph/0505030},
  eprinttype = {arxiv},
  url = {http://arxiv.org/abs/quant-ph/0505030},
  urldate = {2022-03-11},
  abstract = {This pedagogical review presents the proof of the Solovay-Kitaev theorem in the form of an efficient classical algorithm for compiling an arbitrary single-qubit gate into a sequence of gates from a fixed and finite set. The algorithm can be used, for example, to compile Shor's algorithm, which uses rotations of \$\textbackslash pi / 2\^k\$, into an efficient fault-tolerant form using only Hadamard, controlled-\{\textbackslash sc not\}, and \$\textbackslash pi / 8\$ gates. The algorithm runs in \$O(\textbackslash log\^\{2.71\}(1/\textbackslash epsilon))\$ time, and produces as output a sequence of \$O(\textbackslash log\^\{3.97\}(1/\textbackslash epsilon))\$ quantum gates which is guaranteed to approximate the desired quantum gate to an accuracy within \$\textbackslash epsilon {$>$} 0\$. We also explain how the algorithm can be generalized to apply to multi-qubit gates and to gates from \$SU(d)\$.},
  archiveprefix = {arXiv},
  keywords = {Quantum Physics}
}

