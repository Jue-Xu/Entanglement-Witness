@article{lowHamiltonianSimulationQubitization2019,
  title = {Hamiltonian {{Simulation}} by {{Qubitization}}},
  author = {Low, Guang Hao and Chuang, Isaac L.},
  year = {2019},
  month = jul,
  journal = {Quantum},
  volume = {3},
  eprint = {1610.06546},
  eprinttype = {arxiv},
  pages = {163},
  issn = {2521-327X},
  doi = {10.22331/q-2019-07-12-163},
  url = {http://arxiv.org/abs/1610.06546},
  urldate = {2021-04-09},
  abstract = {We present the problem of approximating the time-evolution operator \$e\^\{-i\textbackslash hat\{H\}t\}\$ to error \$\textbackslash epsilon\$, where the Hamiltonian \$\textbackslash hat\{H\}=(\textbackslash langle G|\textbackslash otimes\textbackslash hat\{\textbackslash mathcal\{I\}\})\textbackslash hat\{U\}(|G\textbackslash rangle\textbackslash otimes\textbackslash hat\{\textbackslash mathcal\{I\}\})\$ is the projection of a unitary oracle \$\textbackslash hat\{U\}\$ onto the state \$|G\textbackslash rangle\$ created by another unitary oracle. Our algorithm solves this with a query complexity \$\textbackslash mathcal\{O\}\textbackslash big(t+\textbackslash log(\{1/\textbackslash epsilon\})\textbackslash big)\$ to both oracles that is optimal with respect to all parameters in both the asymptotic and non-asymptotic regime, and also with low overhead, using at most two additional ancilla qubits. This approach to Hamiltonian simulation subsumes important prior art considering Hamiltonians which are \$d\$-sparse or a linear combination of unitaries, leading to significant improvements in space and gate complexity, such as a quadratic speed-up for precision simulations. It also motivates useful new instances, such as where \$\textbackslash hat\{H\}\$ is a density matrix. A key technical result is `qubitization', which uses the controlled version of these oracles to embed any \$\textbackslash hat\{H\}\$ in an invariant \$\textbackslash text\{SU\}(2)\$ subspace. A large class of operator functions of \$\textbackslash hat\{H\}\$ can then be computed with optimal query complexity, of which \$e\^\{-i\textbackslash hat\{H\}t\}\$ is a special case.},
  archiveprefix = {arXiv},
  langid = {english},
  keywords = {Quantum Physics}
}

