  author = {Weilenmann, Mirjam and Dive, Benjamin and Trillo, David and Aguilar, Edgar A. and Navascu{\'e}s, Miguel},
  year = {2020},
  month = may,
  journal = {Phys. Rev. Lett.},
  volume = {124},
  number = {20},
  eprint = {1912.10056},
