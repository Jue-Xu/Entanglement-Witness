@misc{aradAreaLawSubexponential2013,
  title = {An Area Law and Sub-Exponential Algorithm for {{1D}} Systems},
  author = {Arad, Itai and Kitaev, Alexei and Landau, Zeph and Vazirani, Umesh},
  year = {2013},
  month = jan,
  eprint = {1301.1162},
  eprinttype = {arxiv},
  primaryclass = {cond-mat, physics:quant-ph},
  url = {http://arxiv.org/abs/1301.1162},
  urldate = {2022-03-05},
  abstract = {We give a new proof for the area law for general 1D gapped systems, which exponentially improves Hastings' famous result \textbackslash cite\{ref:Has07\}. Specifically, we show that for a chain of d-dimensional spins, governed by a 1D local Hamiltonian with a spectral gap \textbackslash eps{$>$}0, the entanglement entropy of the ground state with respect to any cut in the chain is upper bounded by \$O\{\textbackslash frac\{\textbackslash log\^3 d\}\{\textbackslash eps\}\}\$. Our approach uses the framework Arad et al to construct a Chebyshev-based AGSP (Approximate Ground Space Projection) with favorable factors. However, our construction uses the Hamiltonian directly, instead of using the Detectability lemma, which allows us to work with general (frustrated) Hamiltonians, as well as slightly improving the \$1/\textbackslash eps\$ dependence of the bound in Arad et al. To achieve that, we establish a new, "random-walk like", bound on the entanglement rank of an arbitrary power of a 1D Hamiltonian, which might be of independent interest: \textbackslash ER\{H\^\textbackslash ell\} \textbackslash le (\textbackslash ell d)\^\{O(\textbackslash sqrt\{\textbackslash ell\})\}. Finally, treating d as a constant, our AGSP shows that the ground state is well approximated by a matrix product state with a sublinear bond dimension \$B=e\^\{O(\textbackslash log\^\{3/4\}n/\textbackslash eps\^\{1/4\})\}. Using this in conjunction with known dynamical programing algorithms, yields an algorithm for a 1/\textbackslash poly(n) approximation of the ground energy with a subexponential running time T\textbackslash le \textbackslash exp(e\^\{O(\textbackslash log\^\{3/4\}n/\textbackslash eps\^\{1/4\})\}).},
  archiveprefix = {arXiv},
  langid = {english},
  keywords = {Condensed Matter - Strongly Correlated Electrons,Quantum Physics}
}

