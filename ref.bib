@article{pichlerRealtimeDynamicsLattice2016,
  title = {Real-Time {{Dynamics}} in {{U}}(1) {{Lattice Gauge Theories}} with {{Tensor Networks}}},
  author = {Pichler, T. and Dalmonte, M. and Rico, E. and Zoller, P. and Montangero, S.},
  year = {2016},
  month = mar,
  journal = {Phys. Rev. X},
