@article{babbushExponentiallyMorePrecise2016,
  title = {Exponentially More Precise Quantum Simulation of Fermions {{I}}: {{Quantum}} Chemistry in Second Quantization},
  shorttitle = {Exponentially More Precise Quantum Simulation of Fermions {{I}}},
  author = {Babbush, Ryan and Berry, Dominic W. and Kivlichan, Ian D. and Wei, Annie Y. and Love, Peter J. and {Aspuru-Guzik}, Al{\'a}n},
  year = {2016},
  month = mar,
  journal = {New J. Phys.},
  volume = {18},
  number = {3},
  eprint = {1506.01020},
  eprinttype = {arxiv},
  pages = {033032},
  issn = {1367-2630},
  doi = {10.1088/1367-2630/18/3/033032},
  url = {http://arxiv.org/abs/1506.01020},
  urldate = {2021-05-16},
  abstract = {We introduce novel algorithms for the quantum simulation of molecular systems which are asymptotically more efficient than those based on the Trotter-Suzuki decomposition. We present the first application of a recently developed technique for simulating Hamiltonian evolution using a truncated Taylor series to obtain logarithmic scaling with the inverse of the desired precision, an exponential improvement over all prior methods. The two algorithms developed in this work rely on a second quantized encoding of the wavefunction in which the state of an \$N\$ spin-orbital system is encoded in \$\{\textbackslash cal O\}(N)\$ qubits. Our first algorithm requires at most \$\textbackslash widetilde\{\textbackslash cal O\}(N\^8 t)\$ gates. Our second algorithm involves on-the-fly computation of molecular integrals, in a way that is exponentially more precise than classical sampling methods, by using the truncated Taylor series simulation technique. Our second algorithm has the lowest gate count of any approach to second quantized quantum chemistry simulation in the literature, scaling as \$\textbackslash widetilde\{\textbackslash cal O\}(N\^\{5\} t)\$. The approaches presented here are readily applicable to a wide class of fermionic models, many of which are defined by simplified versions of the chemistry Hamiltonian.},
  archiveprefix = {arXiv},
  keywords = {Physics - Chemical Physics,Quantum Physics}
}

@article{baconAnalyzingAlgebraicQuantum2008,
  title = {Analyzing {{Algebraic Quantum Circuits Using Exponential Sums}}},
  author = {Bacon, Dave and {van Dam}, Wim and Russell, Alexander},
  year = {2008},
  pages = {8},
  url = {https://sites.cs.ucsb.edu/~vandam/LeastAction.pdf},
  abstract = {We introduce and analyze circuits that are the quantum mechanical generalization of classical algebraic circuits. Using the algebraic operations of addition and multiplication, as well as the quantum Fourier transform, such circuits are well-defined for rings Z/mZ and finite fields Fq. The acceptance probabilities of such algebraic quantum circuits can be expressed as exponential sums {$\sum$}x exp(2{$\pi$}i f (x)/m) where the multivariate polynomial f is determined by the circuit, while it is independent of the ring or field over which we interpret the circuit. Dawson et al. [Quantum Information \& Computation, 5(2), pp. 102\textendash 112 (2004)] introduced this ``sum over paths'' description as a discrete version of the path integral approach of standard quantum mechanics. From this perspective, the polynomial f should be interpreted as the ``action'' of a specific (classical) computational path between the input and output of the circuit.},
  langid = {english}
}

@misc{badescuQuantumStateCertification2017,
