  url = {https://iopscience.iop.org/article/10.1088/1751-8113/49/48/484002},
  urldate = {2022-02-22},
  abstract = {An ideal quantum walk transitions from one vertex to another with perfect fidelity, but in physical systems, the particle may be hindered by potential energy barriers. Then the particle has some amplitude of tunneling through the barriers, and some amplitude of staying put. We investigate the algorithmic consequence of such barriers for the quantum walk formulati{$\surd$}on of Grover's algorithm. We prove tha{$\surd$}t the failure amplitude must scale as O(1/ N ) for search to retain its quantum O( N ) runtime; otherwise, it searches in classical O(N ) time. Thus searching larger ``databases'' requires increasingly reliable hop operations or error correction. This condition holds for both discrete- and continuous-time quantum walks.},
  archiveprefix = {arXiv},
  langid = {english}
}

