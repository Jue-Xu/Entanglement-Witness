@article{raussendorfPhaseSpaceSimulation2020,
  title = {Phase Space Simulation Method for Quantum Computation with Magic States on Qubits},
  author = {Raussendorf, Robert and {Bermejo-Vega}, Juani and Tyhurst, Emily and Okay, Cihan and Zurel, Michael},
  year = {2020},
