@misc{cohenGaugeEquivariantConvolutional2019,
  title = {Gauge {{Equivariant Convolutional Networks}} and the {{Icosahedral CNN}}},
  author = {Cohen, Taco S. and Weiler, Maurice and Kicanaoglu, Berkay and Welling, Max},
  year = {2019},
