  eprinttype = {arxiv},
  primaryclass = {quant-ph},
  doi = {10.4230/LIPIcs.TQC.2021.4},
  url = {http://arxiv.org/abs/2011.05733},
  urldate = {2021-11-17},
  abstract = {\$\textbackslash mathsf\{StoqMA\}\$ captures the computational hardness of approximating the ground energy of local Hamiltonians that do not suffer the so-called sign problem. We provide a novel connection between \$\textbackslash mathsf\{StoqMA\}\$ and distribution testing via reversible circuits. First, we prove that easy-witness \$\textbackslash mathsf\{StoqMA\}\$ (viz. \$\textbackslash mathsf\{eStoqMA\}\$, a sub-class of \$\textbackslash mathsf\{StoqMA\}\$) is contained in \$\textbackslash mathsf\{MA\}\$. Easy witness is a generalization of a subset state such that the associated set's membership can be efficiently verifiable, and all non-zero coordinates are not necessarily uniform. This sub-class \$\textbackslash mathsf\{eStoqMA\}\$ contains \$\textbackslash mathsf\{StoqMA\}\$ with perfect completeness (\$\textbackslash mathsf\{StoqMA\}\_1\$), which further signifies a simplified proof for \$\textbackslash mathsf\{StoqMA\}\_1 \textbackslash subseteq \textbackslash mathsf\{MA\}\$ [BBT06, BT10]. Second, by showing distinguishing reversible circuits with ancillary random bits is \$\textbackslash mathsf\{StoqMA\}\$-complete (as a comparison, distinguishing quantum circuits is \$\textbackslash mathsf\{QMA\}\$-complete [JWB05]), we construct soundness error reduction of \$\textbackslash mathsf\{StoqMA\}\$. Additionally, we show that both variants of \$\textbackslash mathsf\{StoqMA\}\$ that without any ancillary random bit and with perfect soundness are contained in \$\textbackslash mathsf\{NP\}\$. Our results make a step towards collapsing the hierarchy \$\textbackslash mathsf\{MA\} \textbackslash subseteq \textbackslash mathsf\{StoqMA\} \textbackslash subseteq \textbackslash mathsf\{SBP\}\$ [BBT06], in which all classes are contained in \$\textbackslash mathsf\{AM\}\$ and collapse to \$\textbackslash mathsf\{NP\}\$ under derandomization assumptions.},
  archiveprefix = {arXiv},
  keywords = {Computer Science - Computational Complexity,Quantum Physics}
}

