  author = {B{\u a}descu, Costin and O'Donnell, Ryan and Wright, John},
  year = {2017},
  month = nov,
  number = {arXiv:1708.06002},
  eprint = {1708.06002},
  eprinttype = {arxiv},
  primaryclass = {quant-ph},
  publisher = {{arXiv}},
  url = {http://arxiv.org/abs/1708.06002},
  urldate = {2022-08-19},
  abstract = {We consider the problem of quantum state certification, where one is given \$n\$ copies of an unknown \$d\$-dimensional quantum mixed state \$\textbackslash rho\$, and one wants to test whether \$\textbackslash rho\$ is equal to some known mixed state \$\textbackslash sigma\$ or else is \$\textbackslash epsilon\$-far from \$\textbackslash sigma\$. The goal is to use notably fewer copies than the \$\textbackslash Omega(d\^2)\$ needed for full tomography on \$\textbackslash rho\$ (i.e., density estimation). We give two robust state certification algorithms: one with respect to fidelity using \$n = O(d/\textbackslash epsilon)\$ copies, and one with respect to trace distance using \$n = O(d/\textbackslash epsilon\^2)\$ copies. The latter algorithm also applies when \$\textbackslash sigma\$ is unknown as well. These copy complexities are optimal up to constant factors.},
  archiveprefix = {arXiv},
  keywords = {Computer Science - Data Structures and Algorithms,Quantum Physics}
}

